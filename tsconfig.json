{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "target": "ES6",
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "app/api/confession/create/route.js", "app/api/posts/[postId]/comment/route.js", "app/api/confession/[confessionId]/delete/route.js", "models/Reaction.js", "app/api/tagUser/route.js", "app/api/payment/create/route.js", "app/api/auth/get-profile/route.js", "app/api/posts/[postId]/like/route.js", "lib/start.js", "actions/getUnlockedConfessions.js"],
  "exclude": ["node_modules"]
}
